{
  "name": "nicsan-crm-backend",
  "version": "1.0.0",
  "description": "Nicsan CRM Backend with Dual Storage (S3 + PostgreSQL)",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node -e \"require('./config/database').initializeDatabase().then(() => process.exit(0)).catch(() => process.exit(1))\"",
    "init-users": "node -e \"require('./services/authService').initializeDefaultUsers().then(() => process.exit(0)).catch(() => process.exit(1))\"",
    "test": "node test-cloud-storage.js",
    "test:cloud": "node test-cloud-storage.js"
  },
  "keywords": [
    "crm",
    "insurance",
    "dual-storage",
    "s3",
    "postgresql"
  ],
  "author": "Nicsan CRM Team",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1450.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^5.16.0",
    "path-to-regexp": "^6.2.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
