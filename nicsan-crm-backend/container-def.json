[
  {
    "name": "backend",
    "image": "132418765186.dkr.ecr.ap-south-1.amazonaws.com/nicsan-crm-api:production-ssl-fix-v2",
    "cpu": 0,
    "portMappings": [
      {
        "containerPort": 3001,
        "hostPort": 3001,
        "protocol": "tcp",
        "name": "backend-3001-tcp",
        "appProtocol": "http"
      }
    ],
    "essential": true,
    "environment": [
      {
        "name": "PGHOST",
        "value": "nicsan-crm-db.cdu4ium6ws7v.ap-south-1.rds.amazonaws.com"
      },
      {
        "name": "PORT",
        "value": "3001"
      },
      {
        "name": "PGUSER",
        "value": "nicsanadmin"
      },
      {
        "name": "PGSSLMODE",
        "value": "require"
      },
      {
        "name": "PGPORT",
        "value": "5432"
      },
      {
        "name": "NODE_ENV",
        "value": "production"
      },
      {
        "name": "JWT_EXPIRES_IN",
        "value": "24h"
      },
      {
        "name": "CORS_ORIGINS",
        "value": "https://app.nicsanin.com,http://localhost:5173"
      },
      {
        "name": "DISABLE_S3",
        "value": "false"
      },
      {
        "name": "FRONTEND_ORIGIN",
        "value": ""
      },
      {
        "name": "FRONTEND_URL",
        "value": "https://app.nicsanin.com"
      }
    ],
    "secrets": [
      {
        "name": "DATABASE_URL",
        "valueFrom": "arn:aws:ssm:ap-south-1:132418765186:parameter/nicsan/backend/DATABASE_URL"
      },
      {
        "name": "JWT_SECRET",
        "valueFrom": "arn:aws:ssm:ap-south-1:132418765186:parameter/nicsan/backend/JWT_SECRET"
      },
      {
        "name": "OPENAI_API_KEY",
        "valueFrom": "arn:aws:ssm:ap-south-1:132418765186:parameter/nicsan/backend/OPENAI_API_KEY"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/nicsan-backend",
        "awslogs-create-group": "true",
        "awslogs-region": "ap-south-1",
        "awslogs-stream-prefix": "backend"
      },
      "secretOptions": []
    },
    "systemControls": []
  }
]